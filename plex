-- StarterGui.pelx.1.mainscr
local function C_35()
local script = G2L["35"];
	local dex = {}
	
	dex.config = {
		mode = "hybrid",
		primaryDecompiler = "unluau",
		secondaryDecompiler = "betterdecompiler",
		currentScript = nil,
		currentInstance = nil,
		scriptCache = {},
		expandedNodes = {}
	}
	
	local classIcons = {
		["Workspace"] = 19, ["Players"] = 21, ["Lighting"] = 22, ["MaterialService"] = 77,
		["ReplicatedFirst"] = 72, ["ReplicatedStorage"] = 73, ["ServerScriptService"] = 0,
		["ServerStorage"] = 0, ["StarterGui"] = 46, ["StarterPack"] = 20, ["StarterPlayer"] = 88,
		["Teams"] = 24, ["SoundService"] = 32, ["LocalScript"] = 18, ["Script"] = 6,
		["ModuleScript"] = 71, ["Part"] = 1, ["MeshPart"] = 60, ["UnionOperation"] = 74,
		["Model"] = 2, ["Folder"] = 79, ["ScreenGui"] = 47, ["Frame"] = 48, ["Default"] = 0
	}
	
	local allProps = {
		"Name", "ClassName", "Parent", "Archivable", "Transparency", "Reflectance", "Color", 
		"Position", "Rotation", "Size", "Anchored", "CanCollide", "CanTouch", "Shape", 
		"Material", "Locked", "CastShadow", "Text", "TextColor3", "TextSize", "Font", 
		"Visible", "Enabled", "ZIndex", "Image", "Texture", "SoundId", "Volume", 
		"PlaybackSpeed", "Playing", "Looped", "Brightness", "Range", "Shadows", 
		"FieldOfView", "WalkSpeed", "JumpPower", "Health", "MaxHealth", "TeamColor"
	}
	
	dex.decompilers = {
		unluau = function(script) return decompile and decompile(script, "unluau") or "-- Error" end,
		medal = function(script) return decompile and decompile(script) or "-- Error" end,
		konstant = function(script) return (Konstant and Konstant.decompile) and Konstant.decompile(script) or "-- Error" end,
		betterdecompiler = function(script) return (syn and syn.decompile) and syn.decompile(script) or "-- Error" end
	}
	
	-- FIX: Reliable way to get clean text for Copy/Save
	function dex.stripTags(txt)
		return txt:gsub("<[^>]*>", ""):gsub("&lt;", "<"):gsub("&gt;", ">")
	end
	
	-- FIX: Highlighting that doesn't break itself
	function dex.highlight(code)
		local colors = {
			kw = "#ff7b72", g = "#d2a8ff", s = "#a5d6ff", 
			n = "#f0883e", op = "#79c0ff", comment = "#8b949e"
		}
	
		-- Escape XML first
		local clean = code:gsub("&", "&amp;"):gsub("<", "&lt;"):gsub(">", "&gt;")
	
		-- We apply highlighting to the code BEFORE adding the <font> tags to avoid double-tagging
		local function color(text, color)
			return string.format('<font color="%s">%s</font>', color, text)
		end
	
		-- 1. Comments (do first)
		clean = clean:gsub("%%%-%%%-.*", function(c) return color(c, colors.comment) end)
	
		-- 2. Strings
		clean = clean:gsub('"[^"]*"', function(s) return color(s, colors.s) end)
		clean = clean:gsub("'[^']*'", function(s) return color(s, colors.s) end)
	
		-- 3. Keywords (with boundaries)
		local kws = {"local", "function", "return", "if", "then", "else", "elseif", "end", "for", "in", "do", "while", "nil", "true", "false", "and", "or", "not", "repeat", "until", "break"}
		for _, w in ipairs(kws) do
			clean = clean:gsub("%f[%w]"..w.."%f[^%w]", function(word)
				return color(word, colors.kw)
			end)
		end
	
		-- 4. Numbers
		clean = clean:gsub("%f[%d]%d+%.?%d*%f[%D]", function(n) return color(n, colors.n) end)
	
		-- 5. Globals
		local globs = {"game", "workspace", "script", "math", "table", "string", "task", "wait", "print", "warn", "error", "Enum", "Instance", "Vector3", "CFrame", "Color3"}
		for _, g in ipairs(globs) do
			clean = clean:gsub("%f[%w]"..g.."%f[^%w]", function(word)
				return color(word, colors.g)
			end)
		end
	
		return clean
	end
	
	local Players = game:GetService("Players")
	local LocalPlayer = Players.LocalPlayer
	local ScreenGui = LocalPlayer:WaitForChild("PlayerGui"):WaitForChild("pelx.1")
	local main = ScreenGui:WaitForChild("main")
	local explorerFrame = main:WaitForChild("explorer")
	local propertiesFrame = main:WaitForChild("properties")
	local viewFrame = main:WaitForChild("view")
	local settingsFrame = main:WaitForChild("settingsf")
	local popupFrame = main:WaitForChild("popup")
	
	local gui = {
		mainframe = main,
		explorerframe = explorerFrame,
		scriptviewerframe = viewFrame,
		settingsframe = settingsFrame,
		propertiespanel = propertiesFrame,
		treeview = explorerFrame:WaitForChild("treeview"),
		searchtextbox = explorerFrame:WaitForChild("search"),
		refreshbutton = explorerFrame:WaitForChild("refresh"),
		propertiesview = propertiesFrame:WaitForChild("prpview"),
		instancenamelabel = propertiesFrame:WaitForChild("instance"),
		scriptscroll = viewFrame:WaitForChild("scroll"),
		scriptdisplay = viewFrame:WaitForChild("scroll"):WaitForChild("display"),
		scripttitle = viewFrame:WaitForChild("title"),
		copybutton = viewFrame:WaitForChild("copy"),
		savebutton = viewFrame:WaitForChild("save"),
		backbutton = viewFrame:WaitForChild("exit"),
		settingsbutton = popupFrame:WaitForChild("setbtn"),
		decompilermode = settingsFrame:WaitForChild("decompiler"),
		primarydecompiler = settingsFrame:WaitForChild("primaryde"),
	}
	
	function dex.buildTree(gui)
		for _, child in ipairs(gui.treeview:GetChildren()) do if child:IsA("GuiObject") then child:Destroy() end end
		local yOffset = 0
		local searchQuery = gui.searchtextbox.Text:lower()
	
		local function addNode(instance, indent)
			if searchQuery ~= "" and not instance.Name:lower():find(searchQuery, 1, true) then return end
			local hasChildren = #instance:GetChildren() > 0
			local isExpanded = dex.config.expandedNodes[instance] or false
			local button = Instance.new("TextButton")
			button.Size, button.Position, button.BackgroundTransparency, button.Text = UDim2.new(1, -5, 0, 20), UDim2.new(0, indent * 16 + 5, 0, yOffset), 1, ""
			button.Parent = gui.treeview
			local arrow = Instance.new("TextLabel")
			arrow.Size, arrow.BackgroundTransparency, arrow.TextColor3, arrow.TextSize, arrow.Parent = UDim2.new(0, 16, 1, 0), 1, Color3.fromRGB(200, 200, 200), 5, button
			arrow.Text = hasChildren and (isExpanded and "▼" or "▶") or ""
			local icon = Instance.new("ImageLabel")
			icon.Size, icon.Position, icon.BackgroundTransparency, icon.Image = UDim2.new(0, 16, 0, 16), UDim2.new(0, 16, 0, 2), 1, "rbxasset://textures/ClassImages.png"
			local idx = classIcons[instance.ClassName] or classIcons["Default"]
			icon.ImageRectOffset, icon.ImageRectSize, icon.Parent = Vector2.new(idx * 16, 0), Vector2.new(16, 16), button
			local name = Instance.new("TextLabel")
			name.Size, name.Position, name.BackgroundTransparency, name.TextColor3, name.TextXAlignment, name.Parent = UDim2.new(1, -36, 1, 0), UDim2.new(0, 36, 0, 0), 1, Color3.fromRGB(220, 220, 220), Enum.TextXAlignment.Left, button
			name.Text = instance.Name
			yOffset = yOffset + 20
			button.MouseButton1Click:Connect(function()
				if hasChildren then dex.config.expandedNodes[instance] = not isExpanded dex.buildTree(gui) end
				dex.selectInstance(instance, gui)
			end)
			local lastClick = 0
			button.MouseButton1Click:Connect(function()
				local now = tick()
				if now - lastClick < 0.3 then
					if instance:IsA("LocalScript") or instance:IsA("ModuleScript") or instance:IsA("Script") then
						dex.viewScript(instance, gui)
					end
				end
				lastClick = now
			end)
			if isExpanded and hasChildren then for _, c in ipairs(instance:GetChildren()) do addNode(c, indent + 1) end end
		end
		local order = {"Workspace", "Players", "Lighting", "MaterialService", "ReplicatedFirst", "ReplicatedStorage", "ServerScriptService", "ServerStorage", "StarterGui", "StarterPack", "StarterPlayer", "Teams", "SoundService", "TextChatService"}
		for _, s in ipairs(order) do local ok, serv = pcall(game.GetService, game, s) if ok and serv then addNode(serv, 0) end end
		gui.treeview.CanvasSize = UDim2.new(0, 0, 0, yOffset + 10)
	end
	
	function dex.selectInstance(instance, gui)
		dex.config.currentInstance = instance
		gui.instancenamelabel.Text = instance.ClassName .. ' "' .. instance.Name .. '"'
		for _, child in ipairs(gui.propertiesview:GetChildren()) do if child:IsA("GuiObject") then child:Destroy() end end
		local ypos = 5
		for _, propName in ipairs(allProps) do
			local success, val = pcall(function() return instance[propName] end)
			if success and val ~= nil then
				local f = Instance.new("Frame")
				f.Size, f.Position, f.BackgroundTransparency, f.Parent = UDim2.new(1, -14, 0, 20), UDim2.new(0, 0, 0, ypos), 1, gui.propertiesview
				local n = Instance.new("TextLabel")
				n.Size, n.Text, n.TextColor3, n.BackgroundTransparency, n.TextXAlignment, n.Parent = UDim2.new(0.4, 0, 1, 0), propName, Color3.fromRGB(150,150,150), 1, Enum.TextXAlignment.Left, f
				local v = Instance.new("TextLabel")
				v.Size, v.Position, v.Text, v.TextColor3, v.BackgroundTransparency, v.TextXAlignment, v.Parent = UDim2.new(0.6, 0, 1, 0), UDim2.new(0.4, 0, 0, 0), tostring(val), Color3.fromRGB(200,200,200), 1, Enum.TextXAlignment.Left, f
				ypos = ypos + 20
			end
		end
		gui.propertiesview.CanvasSize = UDim2.new(0, 0, 0, ypos + 5)
	end
	
	function dex.viewScript(script, gui)
		dex.config.currentScript = script
		gui.scriptviewerframe.Visible = true
		gui.explorerframe.Visible, gui.propertiespanel.Visible, gui.settingsframe.Visible = false, false, false
		gui.scripttitle.Text = script:GetFullName()
		gui.scriptdisplay.RichText = true
	
		local function updateDisplay(txt)
			gui.scriptdisplay.Text = dex.highlight(txt)
			local bounds = gui.scriptdisplay.TextBounds
			gui.scriptdisplay.Size = UDim2.new(1, 0, 0, bounds.Y + 20)
			gui.scriptscroll.CanvasSize = UDim2.new(0, bounds.X + 20, 0, bounds.Y + 40)
		end
	
		if not dex.config.scriptCache[script] then
			gui.scriptdisplay.Text = "Decompiling..."
			task.spawn(function()
				local s, r = pcall(function() return dex.decompile(script) end)
				local final = s and r or "-- Decompilation Failed"
				dex.config.scriptCache[script] = final
				updateDisplay(final)
			end)
		else
			updateDisplay(dex.config.scriptCache[script])
		end
	end
	
	-- BUTTONS
	gui.copybutton.MouseButton1Click:Connect(function()
		if setclipboard then setclipboard(dex.stripTags(gui.scriptdisplay.Text)) end
	end)
	
	gui.savebutton.MouseButton1Click:Connect(function()
		if writefile and dex.config.currentScript then
			writefile(dex.config.currentScript.Name..".lua", dex.stripTags(gui.scriptdisplay.Text))
		end
	end)
	
	gui.refreshbutton.MouseButton1Click:Connect(function() dex.buildTree(gui) end)
	gui.settingsbutton.MouseButton1Click:Connect(function() 
		gui.settingsframe.Visible = not gui.settingsframe.Visible 
		if gui.settingsframe.Visible then gui.settingsframe.ZIndex = 10 end
	end)
	gui.backbutton.MouseButton1Click:Connect(function()
		gui.explorerframe.Visible, gui.propertiespanel.Visible, gui.scriptviewerframe.Visible = true, true, false
	end)
	gui.searchtextbox:GetPropertyChangedSignal("Text"):Connect(function() dex.buildTree(gui) end)
	
	dex.config.expandedNodes[game] = true
	dex.buildTree(gui)
end;
task.spawn(C_35);

return G2L["1"], require;
